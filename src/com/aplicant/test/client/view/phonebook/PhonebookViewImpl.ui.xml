<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="627px">
			<g:HorizontalPanel>
				<g:Button ui:field="addButton">Добавить</g:Button>
				<g:Button ui:field="viewButtom">Просмотр</g:Button>
				<g:Button ui:field="deleteButtom">Удалить</g:Button>
				<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
					<g:Label text="Фильтр" horizontalAlignment="ALIGN_CENTER" height="28px"/>
				</g:Cell>
				<g:TextBox ui:field="filterInput"/>
				<g:Button ui:field="clearButton">Очистить</g:Button>
			</g:HorizontalPanel>
			<g:ListBox visibleItemCount="15" width="627px" height="332px" ui:field="contactList"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 